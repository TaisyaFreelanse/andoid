  "config": {
    "backup_before": true,
    "reboot_after": false,
    "verify_changes": true,
    "auto_detect_country": true,
    "use_proxy_geolocation": true,
    "proxy_selection": "random",
    "browser": "webview"
  },

  "actions": [
    {
      "id": "step1_setup_proxy",
      "type": "setup_proxy",
      "proxy": "random",
      "description": "Шаг 1: Установка случайного SOCKS5 прокси"
    },
    {
      "id": "step2_detect_proxy_location",
      "type": "detect_proxy_location",
      "description": "Шаг 2: Определение геолокации прокси (город, штат, координаты)"
    },
    {
      "id": "step3_change_timezone",
      "type": "change_timezone",
      "timezone": "auto",
      "country_code": "US",
      "description": "Шаг 3: Установка таймзоны на основе прокси (авто)"
    },
    {
      "id": "step4_change_location",
      "type": "change_location",
      "latitude": "auto",
      "longitude": "auto",
      "description": "Шаг 4: Подмена геолокации на основе прокси"
    },
    {
      "id": "step5_change_locale",
      "type": "change_locale",
      "locale": "en_US",
      "description": "Шаг 5: Установка английского языка (США)"
    },
    {
      "id": "step6_clear_browser_data",
      "type": "clear_webview_data",
      "description": "Шаг 6: Очистка данных WebView/браузера"
    },
    {
      "id": "step7_open_google",
      "type": "open_url",
      "url": "https://www.google.com",
      "wait_ms": 3000,
      "description": "Шаг 7: Открытие Google"
    },
    {
      "id": "step8_google_search",
      "type": "google_search",
      "query": "site:pickuplineinsider.com",
      "wait_ms": 5000,
      "description": "Шаг 8: Поиск в Google"
    },
    {
      "id": "step9_click_random_result",
      "type": "click_search_result",
      "result_index": "random",
      "wait_ms": 5000,
      "description": "Шаг 9: Клик по случайной ссылке из результатов"
    },
    {
      "id": "step10_browse_page",
      "type": "browse_page",
      "scroll": true,
      "scroll_count": 3,
      "stay_time_ms": 10000,
      "description": "Шаг 10: Просмотр страницы (скролл, задержка)"
    },
    {
      "id": "step11_regenerate_android_id",
      "type": "regenerate_android_id",
      "description": "Шаг 11: Регенерация Android ID"
    },
    {
      "id": "step12_regenerate_aaid",
      "type": "regenerate_aaid",
      "description": "Шаг 12: Регенерация AAID (Advertising ID)"
    },
    {
      "id": "step13_change_user_agent",
      "type": "change_user_agent",
      "ua": "random",
      "description": "Шаг 13: Изменение User-Agent на случайный"
    },
    {
      "id": "step14_modify_fingerprint",
      "type": "modify_fingerprint",
      "description": "Шаг 14: Изменение fingerprint устройства"
    }
  ],

  "post_process": {
    "verify_android_id_changed": true,
    "verify_user_agent_changed": true,
    "verify_timezone_matches_proxy": true,
    "verify_location_matches_proxy": true,
    "log_new_fingerprint": true,
    "send_to_backend": true,
    "screenshot_after": true
  },

  "error_handling": {
    "continue_on_error": true,
    "max_retries": 3,
    "retry_delay_ms": 5000,
    "notify_on_failure": true
  }
}
