  "type": "uniqueness",
  "requires_root": true,
  "timeout": 180000,
  "config": {
    "backup_before": true,
    "reboot_after": false,
    "verify_changes": true,
    "log_detailed": true,
    "auto_detect_country": true,
    "use_proxy_geolocation": true,
    "proxy_rotation_interval": 600000,
    "proxy_rotation_enabled": true
