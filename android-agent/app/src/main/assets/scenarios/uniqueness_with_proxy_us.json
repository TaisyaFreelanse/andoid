  "actions": [
    {
      "id": "action_1",
      "type": "detect_proxy_location",
      "save_as": "proxy_location",
      "description": "Определение геолокации прокси (страна, штат, timezone)"
    },
    {
      "id": "action_2",
      "type": "regenerate_android_id",
      "description": "1. Регенерация AndroidID через settings put secure android_id"
    },
    {
      "id": "action_3",
      "type": "regenerate_aaid",
      "description": "2. Регенерация AAID через settings put secure android_id"
    },
    {
      "id": "action_4",
      "type": "clear_chrome_data",
      "description": "3. Очистка Chrome data через pm clear и удаление папок"
    },
    {
      "id": "action_5",
      "type": "clear_webview_data",
      "description": "4. Очистка WebView data через pm clear и удаление папок"
    },
    {
      "id": "action_6",
      "type": "change_user_agent",
      "ua": "random",
      "locale": "en_US",
      "description": "5. Изменение User-Agent через build.prop или runtime"
    },
    {
      "id": "action_7",
      "type": "change_timezone",
      "timezone": "auto",
      "country_code": "US",
      "use_proxy_timezone": true,
      "description": "6. Автоматическая установка timezone по геолокации прокси (settings put global auto_time_zone)"
    },
    {
      "id": "action_8",
      "type": "change_location",
      "latitude": "auto",
      "longitude": "auto",
      "country_code": "US",
      "use_proxy_location": true,
      "description": "7. Автоматическая установка GPS координат по геолокации прокси (settings put secure mock_location)"
    },
    {
      "id": "action_9",
      "type": "modify_build_prop",
      "params": {
        "ro.product.model": "random",
        "ro.product.manufacturer": "random",
        "ro.product.brand": "random"
      },
      "description": "8. Изменение build.prop параметров (model, manufacturer, etc.)"
    },
    {
      "id": "action_10",
      "type": "change_locale",
      "locale": "en_US",
      "description": "9. Изменение локали устройства на en_US"
    }
  ],
  "post_process": {
    "verify_android_id_changed": true,
    "verify_aaid_changed": true,
    "verify_user_agent_changed": true,
    "verify_timezone_matches_proxy": true,
    "verify_location_matches_proxy": true,
